module structsDemo

// Structs Creation
struct One {
    item1: String,
    item2: Int
}

struct Two {
    function: fn(Int) -> Int
}

// You can use () or {} to construct a struct
let thingone : One = One("wow",3)
let thingtwo = One {item1: "hello",item2: 30}
let thingthree = Two(fn(x:Int) -> Int {return x * x})
